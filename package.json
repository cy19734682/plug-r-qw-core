{
  "name": "@zhangqingcq/plug-r-qw",
  "version": "2.2.4",
  "description": "A JS lib base on Vue@3.x and view-ui-plus, you can achieve some complex functions with simple code after install this lib.",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "files": [
    "lib",
    "src",
    "types"
  ],
  "main": "./lib/plugRQw.js",
  "module": "./lib/plugRQw.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./lib/plugRQw.js",
      "require": "./lib/plugRQw.umd.cjs"
    },
    "./lib/**/*.js": "./lib/**/*.js",
    "./*": "./*"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build --mode pro",
    "type-check": "vue-tsc --build --force",
    "types": "vue-tsc -p tsconfig.app.declaration.json",
    "build-types": "api-extractor run --local --verbose && gulp clean_temp",
    "node-serve": "node node-serve/index.cjs",
    "node-serve-dev": "nodemon node-serve/index.cjs",
    "lib": "vue-tsc --noEmit --skipLibCheck && vite build --mode lib && gulp lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RickyHeaven/plug-r-qw-core.git"
  },
  "keywords": [
    "vue",
    "view-ui-plus",
    "vite",
    "typescript",
    "vue-router",
    "pinia",
    "sweetalert",
    "lodash",
    "axios",
    "echarts",
    "moment",
    "wangeditor",
    "xss",
    "js-cookie",
    "iview-area",
    "ar-cascader",
    "vue-json-viewer",
    "qw",
    "ricky",
    "zhangqingcq"
  ],
  "author": "RickyHeaven",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RickyHeaven/plug-r-qw-core/issues"
  },
  "homepage": "https://github.com/RickyHeaven/plug-r-qw-core#readme",
  "dependencies": {
    "@amap/amap-jsapi-loader": "^1.0.1",
    "@types/lodash-es": "^4.17.8",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-vue": "^5.1.12",
    "ar-cascader": "^2.1.1",
    "axios": "1.6.8",
    "deepmerge": "^4.3.1",
    "js-cookie": "3.0.5",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.4",
    "popper.js": "1.16.1-lts",
    "sweetalert": "^2.1.2",
    "view-ui-plus": "^1.3.16",
    "vue": "^3.4.21",
    "vue-json-viewer": "^3.0.4"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.43.1",
    "@tsconfig/node20": "^20.1.4",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^20.12.5",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/tsconfig": "^0.5.1",
    "@zhangqingcq/view-ui-plus-resolver": "^0.0.1",
    "del": "^6.1.1",
    "express": "^4.18.2",
    "github-markdown-css": "^5.2.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-less": "^5.0.0",
    "gulp-rename": "^2.0.0",
    "less": "^4.2.0",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.7",
    "multer": "1.4.5-lts.1",
    "nodemon": "^2.0.22",
    "npm-run-all2": "^6.1.2",
    "pinia": "^2.1.7",
    "prettier": "^3.0.0",
    "typescript": "~5.4.0",
    "unplugin-auto-import": "^0.17.2",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^5.2.8",
    "vite-plugin-vue-devtools": "^7.0.25",
    "vue-i18n": "^9.12.1",
    "vue-router": "^4.3.0",
    "vue-tsc": "^2.0.13"
  },
  "engines": {
    "node": ">=20.3.1",
    "npm": ">=9.6.7",
    "pnpm": ">=8.6.7"
  }
}
