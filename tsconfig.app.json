{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "auto-imports.d.ts",
    "./src/**/*",
    "./src/**/*.ts",
    "./examples/**/*.json",
    "./examples/**/*",
  ],
  "exclude": [],
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "composite": true,
    "moduleResolution": "node",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "paths": {
      "@/*": [
        "./examples/*"
      ]
    }
  }
}
